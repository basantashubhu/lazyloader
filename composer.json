{
    "name": "basanta/lazyloader",
    "description": "Lazy load Laravel Models with or without relationship.",
    "keywords": [
        "laravel",
        "eloquent",
        "relationship",
        "lazy load"
    ],
    "homepage": "https://github.com/basantashubhu/lazyloader",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Basanta\\LazyLoader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Basanta\\LazyLoader\\LazyLoaderServiceProvider"
            ],
            "aliases": {
                "LazyLoader": "Basanta\\LazyLoader\\LazyLoader"
            }
        }
    },
    "authors": [
        {
            "name": "basantashubhu",
            "email": "callmebasanta44@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=10.0"
    }
}